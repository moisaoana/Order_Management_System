<AnchorPane prefHeight="406.0" prefWidth="539.0" style="-fx-background-color: aliceblue;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.presentation.ClientsWindow">
