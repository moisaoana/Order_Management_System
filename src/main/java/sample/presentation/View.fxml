<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="445.0" prefWidth="465.0" style="-fx-background-color: aliceblue;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.presentation.Controller">
   <children>
      <Label fx:id="titleLabel" layoutX="98.0" layoutY="90.0" prefHeight="39.0" prefWidth="311.0" style="-fx-text-fill: cornflowerblue;" text="Order Management">
         <font>
            <Font name="Berlin Sans FB" size="35.0" />
         </font>
      </Label>
      <Button fx:id="buttonClients" layoutX="177.0" layoutY="164.0" mnemonicParsing="false" onAction="#clickClients" prefHeight="39.0" prefWidth="111.0" style="-fx-background-color: orangered;" text="Clients" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="21.0" />
         </font>
      </Button>
      <Button fx:id="buttonProducts" layoutX="177.0" layoutY="245.0" mnemonicParsing="false" onAction="#clickProducts" prefHeight="39.0" prefWidth="111.0" style="-fx-background-color: orangered;" text="Products" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="21.0" />
         </font>
      </Button>
      <Button fx:id="buttonOrders" layoutX="177.0" layoutY="321.0" mnemonicParsing="false" onAction="#clickOrders" prefHeight="39.0" prefWidth="111.0" style="-fx-background-color: orangered;" text="Orders" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
